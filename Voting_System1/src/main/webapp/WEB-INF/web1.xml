<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>AdminLoginServlet</servlet-name>
        <servlet-class>Voting_System1.AdminLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminLoginServlet</servlet-name>
        <url-pattern>/AdminLoginServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>VoterLoginServlet</servlet-name>
        <servlet-class>Voting_System1.VoterLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VoterLoginServlet</servlet-name>
        <url-pattern>/VoterLoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterVoterServlet</servlet-name>
        <servlet-class>Voting_System1.RegisterVoterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterVoterServlet</servlet-name>
        <url-pattern>/RegisterVoterServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>AddCandidateServlet</servlet-name>
        <servlet-class>Voting_System1.AddCandidateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddCandidateServlet</servlet-name>
        <url-pattern>/AddCandidateServlet</url-pattern>
    </servlet-mapping>

<servlet>
    <servlet-name>CastVoteServlet</servlet-name>
    <servlet-class>Voting_System1.CastVoteServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>CastVoteServlet</servlet-name>
    <url-pattern>/CastVoteServlet</url-pattern>
</servlet-mapping>

    
    <servlet>
        <servlet-name>AdminLogoutServlet</servlet-name>
        <servlet-class>Voting_System1.AdminLogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminLogoutServlet</servlet-name>
        <url-pattern>/AdminLogoutServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>VoterLogoutServlet</servlet-name>
        <servlet-class>Voting_System1.VoterLogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VoterLogoutServlet</servlet-name>
        <url-pattern>/VoterLogoutServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
    <servlet-name>ViewResultsServlet</servlet-name>
    <servlet-class>Voting_System1.ViewResultsServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ViewResultsServlet</servlet-name>
    <url-pattern>/ViewResultsServlet</url-pattern>
</servlet-mapping>
    
</web-app>
